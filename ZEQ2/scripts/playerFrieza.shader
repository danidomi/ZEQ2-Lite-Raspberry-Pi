frieza1Upper
{
	outlines
	nomipmaps
	{
		map players/frieza/frieza1Shine.png
		rgbGen identityLighting
		tcGen environment
	} 
	{
		map players/frieza/frieza1Upper.png
		blendFunc blend
		rgbGen identityLighting
	}
	{
		clampMap effects/shading/celShadeDark3Tone.png
		blendfunc filter
		rgbGen lightingUniform
		tcGen cel
	}
}

